{
  "name": "lucid-ui",
  "version": "2.13.0-path-imports.0",
  "description": "A UI component library from AppNexus.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/appnexus/lucid.git"
  },
  "scripts": {
    "build-bundle": "scripts/build-bundle.sh",
    "cover": "npm run jest -- --coverage",
    "dev": "gulp docs-generate && webpack-dev-server --inline --content-base src/docs/",
    "docs-upload-gh": "scripts/docs-upload-gh.sh",
    "docs-upload": "gulp docs-upload",
    "jest": "NODE_ENV=test TZ=UTC jest",
    "lint": "eslint . --ext .js,.jsx",
    "lint-fix": "eslint . --ext .js,.jsx --fix",
    "preversion": "rm -rf dist && npm run lint && gulp preversion && npm run build-bundle",
    "postversion": "gulp build-js-clean-path-imports && gulp docs-upload",
    "start": "npm run dev",
    "test": "npm run lint && npm run cover",
    "watch": "npm run jest -- --watch"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "keywords": [
    "component",
    "ecosystem-react",
    "react",
    "react-component",
    "reactive",
    "stateful",
    "stateless"
  ],
  "author": "AppNexus engineering",
  "license": "Apache-2.0",
  "dependencies": {
    "classnames": "^2.2.3",
    "d3-format": "^1.0.2",
    "d3-scale": "^1.0.0",
    "d3-shape": "^1.0.0",
    "d3-time": "^1.0.0",
    "d3-time-format": "^2.0.0",
    "element-resize-detector": "^1.1.6",
    "lodash": "^4.11.0",
    "react-motion": "^0.4.3",
    "reselect": "^2.5.1"
  },
  "devDependencies": {
    "anx-docgen": "^1.1.0",
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.5.0",
    "babylon": "^6.0.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "^6.7.1",
    "file-loader": "^0.8.5",
    "form-data": "^0.2.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-gzip": "^1.2.0",
    "gulp-less": "^3.0.5",
    "gulp-postcss": "^6.1.0",
    "gulp-tar": "^1.8.0",
    "history": "^2.0.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "less": "^2.3.1",
    "less-loader": "^2.2.2",
    "markdown": "^0.5.0",
    "postcss-loader": "^0.8.1",
    "pre-push": "^0.1.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.0.0",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "jest": {
    "rootDir": "src",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/examples/**",
      "!docs/**",
      "!*.*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
