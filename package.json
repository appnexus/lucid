		"sinon": "^4.5.0",
		"storybook-chromatic": "^3.5.5",
		"style-loader": "^0.20.3",
		"stylelint": "^13.13.1",
		"stylelint-config-standard": "^20.0.0",
		"timemachine": "^0.3.0",
		"ts-loader": "^6.0.4",
		"typescript": "^4.3.5",
		"webpack": "^4.27.1",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10"
	},
	"peerDependencies": {
		"react": "^15.0.0 || ^16.0.0",
		"react-dom": "^15.0.0 || ^16.0.0"
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run lint && npm test"
		}
	}
}
