		"react-dom": "^16.6.3",
		"sinon": "^13.0.1",
		"style-loader": "^2.0.0",
		"stylelint": "^14.3.0",
		"stylelint-config-standard": "^20.0.0",
		"timekeeper": "^2.2.0",
		"timemachine": "^0.3.2",
		"ts-loader": "^8.3.0",
		"typescript": "^4.5.5",
		"webpack": "^4.46.0",
		"webpack-cli": "^4.9.2"
	},
	"peerDependencies": {
		"react": "^15.0.0 || ^16.0.0",
		"react-dom": "^15.0.0 || ^16.0.0"
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run lint && npm test"
		}
	}
}
